/* pages/feed/feed.wxss */
.search {
    position: fixed;
    top: 30rpx;
    right: 25rpx;
    width: 200rpx;
    height: 50rpx;
    display: block;
    box-sizing: border-box;
}
.search input {
    display: block;
    box-sizing: border-box;
    background: rgba(255, 255, 255, 0.3);
    border: 1rpx solid rgba(255, 255, 255, 0.3);
    width: 200rpx;
    height: 50rpx;
    border-radius: 20rpx;
    padding-left: 10rpx; 
}